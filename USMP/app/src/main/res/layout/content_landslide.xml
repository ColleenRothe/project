                android:layout_width="20dp"
                android:layout_height="wrap_content"
                android:text=":"
                />
            <EditText
                android:id="@id/L_DitchSlope4"
                android:layout_width="100dp"
                android:inputType="numberDecimal"
                android:layout_height="wrap_content"
                />

